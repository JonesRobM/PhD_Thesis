@ARTICLE{Carter2022-bn,
  title    = "Reversible Growth of Gold Nanoparticles in the {Low-Temperature}
              {Water-Gas} Shift Reaction",
  author   = "Carter, James H and Abdel-Mageed, Ali M and Zhou, Dan and Morgan,
              David J and Liu, Xi and Bansmann, Joachim and Chen, Shilong and
